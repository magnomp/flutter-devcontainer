{
	"name": "Flutter",
	"dockerComposeFile": "docker-compose.yml",
	"service": "dev",
	"workspaceFolder": "/workspace",
	"customizations": {
		"vscode": {
			"settings": {
				"terminal.integrated.profiles.linux": {
					"bash": {
						"path": "/bin/bash",
						"icon": "terminal-bash"
					}
				},
				"terminal.integrated.defaultProfile.linux": "bash",		
			},
			"extensions": [
				"dart-code.dart-code",
				"dart-code.flutter",
				"toroxx.vscode-avdmanager"
			]
		}
	},
	
	"remoteUser": "vscode",
}